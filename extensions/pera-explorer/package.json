{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "pera-explorer",
  "title": "Pera Algorand Explorer",
  "description": "Search any ASA or NFT in Algorand (ALGO) Blockchain",
  "icon": "pera-icon.png",
  "author": "akgunberk",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search ASA",
      "subtitle": "Algorand",
      "description": "Search any ASA or NFT on Algorand",
      "mode": "view"
    }
  ],
  "categories": [
    "Finance",
    "Web"
  ],
  "dependencies": {
    "@raycast/api": "^1.57.1",
    "@raycast/utils": "^1.3.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^3.0.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
